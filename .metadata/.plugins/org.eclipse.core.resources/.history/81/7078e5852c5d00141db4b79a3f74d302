package DoubleLinkedList;

public class Driver {

	public Driver() {
		LinkedList l = new LinkedList();
		Node front = new Node(5);
		l.insertAfterTheFront(6,front);
		l.insertAfterTheFront(4,front);
		l.insertAfterTheFront(4,front);
		System.out.println(l.sizeOfLinkedList(front));
	}

	public static void main(String[] args) {
		new Driver();

	}

}
