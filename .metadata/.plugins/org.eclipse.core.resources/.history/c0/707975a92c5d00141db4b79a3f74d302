package DoubleLinkedList;

public class Driver {

	public Driver() {
		LinkedList l = new LinkedList();
		Node front = new Node(0);
		l.insertAfterTheFront(3,front);
		l.insertAfterTheFront(2,front);
		l.insertAfterTheFront(1,front);
		System.out.println(l.sizeOfLinkedList(front));
	}

	public static void main(String[] args) {
		new Driver();

	}

}
