package MyLinkedList;

public class Driver {

	public Driver() {
		LinkedList l = new LinkedList();
		l.addFront(10);
		l.addFront(11);
		l.addFront(12);
		
		System.out.println(l.sizeOfList());
		l.removeNode();
		System.out.println(l.sizeOfList());
	}

	public static void main(String[] args) {
		new Driver();
	}

}
