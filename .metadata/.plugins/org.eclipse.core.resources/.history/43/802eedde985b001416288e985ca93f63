	}
	
	public boolean closeEnough(double a, double b){
		
	}
	
	private static double mySqaureRoot(double number){
		
		double lower = 0, upper = number;
		while(lower<upper){
			double guess = (lower + upper) / 2;
			double square = guess*guess;
			if(square < number)
				lower = guess;
			if(square > number)
				upper = guess;
			
			
		}
		
		
		return 0.0;
	}

}
